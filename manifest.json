{
  "version": 3,
  "name": "Remove Google AI Overview when searching.",
  "version": "0.0.1",
  "decription": "Stops certain requests, and removes Google AI Overview components.",
  "permissions": [
    "declarativeNetRequestFeedback",
    "declarativeNetRequest"
  ],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_0_",
      "enabled": true,
      "path": "rules_gen_24.json"
    }]
  },
  "host_permissions": [
    "*://*.google.com/search"
  ],
}
